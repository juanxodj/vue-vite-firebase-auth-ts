    "firebase": "^9.9.0",
    "moment": "^2.29.4",
    "nanoid": "^4.0.0",
    "pinia": "^2.0.14",
    "unplugin-vue-components": "^0.21.1",
    "uuid": "^8.3.2",
    "vue": "^3.2.25",
    "vue-router": "^4.1.1",
    "vue-sweetalert2": "^5.0.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@types/uuid": "^8.3.4",
